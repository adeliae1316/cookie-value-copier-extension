{
  "name": "Cookie value copier",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Copy specified cookie value to clipboard.",
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "activeTab",
    "cookies",
    "storage",
    "clipboardWrite"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "192": "icon/192.png"
  }
}